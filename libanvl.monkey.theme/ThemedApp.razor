<Router AppAssembly="@typeof(ThemedApp).Assembly">
    <Found Context="routeData">
        <RouteView RouteData="@routeData" DefaultLayout="@typeof(MainLayout)" />
        <FocusOnNavigate RouteData="@routeData" Selector=".title" />
    </Found>
    <NotFound>
        <PageTitle>Not found</PageTitle>
        <LayoutView Layout="@typeof(SingleLayout)">
              <libanvl.monkey.theme.Parts.Post Title="Sorry" SubTitle="There's nothing at this address" EnableActions="@false" TitleHref="" />
        </LayoutView>
    </NotFound>
</Router>
